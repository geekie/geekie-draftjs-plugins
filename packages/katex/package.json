{
  "name": "@geekie/geekie-katex",
  "version": "0.0.5",
  "sideEffects": [
    "*.css"
  ],
  "description": "Geekie Katex Plugin for DraftJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/geekie/geekie-draftjs-plugins"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "katex",
    "latex",
    "react-component"
  ],
  "scripts": {
    "clean": "yarn rimraf lib",
    "build": "yarn build:js && yarn build:dts && yarn build:css",
    "build:js": "yarn rollup --config ../../rollup.config.js",
    "build:dts": "tsc -d --emitDeclarationOnly --declarationDir lib",
    "build:css": "node ../../scripts/build-css.js $(pwd)",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "katex": "0.16.0",
    "math-input-web-support": "0.2.4",
    "mathquill-commonjs": "0.10.3"
  },
  "devDependencies": {
    "@types/katex": "0.14.0"
  },
  "peerDependencies": {
    "draft-js": "^0.10.1 || ^0.11.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/geekie/geekie-draftjs-plugins/tree/main/packages/katex",
  "directories": {
    "lib": "lib"
  }
}
